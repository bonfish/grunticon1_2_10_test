{{#each customselectors}}{{this}},{{/each}}
{{prefix}}{{name}}{
	display:inline-block;
	background-image:url('{{datauri}}');
	background-repeat:no-repeat;{{#width}}
	width:{{this}};{{/width}}{{#height}}
	height:{{this}};{{/height}}
	content:""; // использовать осторожно. Ломает текст в старых Опера, анимацию в старых Хромах
}